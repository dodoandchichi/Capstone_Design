   "source": [
    "import numpy as np\n",
    "from scipy.io.wavfile import write\n",
    "\n",
    "# CSV 파일에서 소음 데이터 로드\n",
    "data = np.loadtxt('sound_data.csv', delimiter=',')\n",
    "\n",
    "# 소음 데이터를 WAV 파일 저장\n",
    "write('sound_data.wav', 44100, data.astype(np.int16))\n"
   ]
  }
